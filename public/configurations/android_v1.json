{
  "settings": {},
  "rules": [
    {
      "patterns": [
        "/headache_logs$",
        "/charts$",
        "/settings$",
        "/feedback$"
      ],
      "properties": {
        "context": "default",
        "uri": "hotwire://fragment/web",
        "presentation": "replace"
      }
    },
    {
      "patterns": [
        "/headache_logs/new$",
        "/headache_logs/[0-9]+/edit$"
      ],
      "properties": {
        "context": "modal",
        "uri": "hotwire://fragment/web/modal/sheet",
        "pull_to_refresh_enabled": false
      }
    }
  ]
}
